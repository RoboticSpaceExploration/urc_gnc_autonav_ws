<launch>
    <rosparam file="$(find gnc_localization)/launch/ekf_config.yaml"/>
    <!--
	<node pkg="robot_localization" type="ekf_localization_node" name="local_ekf_localization">
        <param name="map_frame" value="map"/>
	<param name="odom_frame" value="odom"/>
        <param name="base_link_frame" value="base_link"/>
        <param name="world_frame" value="map"/>
    </node>
    -->
    <node pkg="robot_localization" type="ekf_localization_node" name="global_ekf_localization">
        <param name="map_frame" value="map"/>
	<param name="odom_frame" value="odom"/>
        <param name="base_link_frame" value="base_link"/>
        <param name="world_frame" value="map"/>
    </node>
    <!-- Navsat Transform Node -->
    <rosparam file="$(find gnc_localization)/launch/navsat_params.yaml"/>
    <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform">
	<remap from="gps/fix" to="/gps/fix/time"/>
	<remap from="/imu/data" to="imu/data"/>
	<remap from="odom" to="/odometry/filtered"/>
    </node>
</launch>

